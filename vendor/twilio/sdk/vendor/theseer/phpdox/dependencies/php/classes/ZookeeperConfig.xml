<?xml version="1.0" encoding="UTF-8" ?>
<class xmlns="http://xml.phpdox.net/src" full="ZookeeperConfig" namespace="" name="ZookeeperConfig">
  <method name="add" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Add servers to the ensemble"/>
      <return type="void"/>
    </docblock>
    <parameter name="members" optional="false" byreference="false" type="string"/>
    <parameter name="version" optional="true" byreference="false" type="int"/>
    <parameter name="stat" optional="true" byreference="false" type="object" class="array"/>
  </method>
  <method name="get" abstract="false" static="false" visibility="public" final="false">
